}

.ribbon .ribbon-piece {
  background-color: rgba(102, 102, 102, 1);
  -moz-box-shadow: 0 0 5px rgba(0, 0, 0, 0.2);
-ms-box-shadow: 0 0 5px rgba(0, 0, 0, 0.2);
